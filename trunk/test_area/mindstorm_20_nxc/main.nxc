#include "_3DFrame.h"

task main()
{  
  _3DFrame F;
  
  _3D_translate( F,  1, 2, 3 );
  
  _3D_rot_x( F, _PI );
  
  print( F );
  
  _3D_rot_x( F, -_PI );
  
  print( F );  
}
